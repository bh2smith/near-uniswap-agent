{
  "name": "bitte-uniswap-agent",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/app.ts",
    "build": "rm -rf dist && tsc",
    "start:dist": "node dist/src/app.js",
    "dev": "concurrently \"tsx src/app.ts\" \"make-agent dev\"",
    "dev-testnet": "concurrently \"tsx src/app.ts\" \"make-agent dev -t\"",
    "serve": "node dist/api/index.js",
    "lint": "biome check && prettier --check '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "biome format --write . && prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "deploy": "make-agent deploy -u https://near-uniswap-agent.vercel.app/",
    "test": "jest --config jest.config.ts",
    "vercel-build": "echo hello"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.3.0",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/smart-order-router": "^4.22.20",
    "cors": "^2.8.5",
    "ethers": "=5.7.2",
    "express": "^5.1.0",
    "near-safe": "0.11.0",
    "viem": "^2.35.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/swagger-ui-express": "^4.1.8",
    "biome": "^0.3.3",
    "concurrently": "^9.2.1",
    "dotenv": "17.2.1",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "make-agent": "^0.3.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
