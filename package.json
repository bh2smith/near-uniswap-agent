{
  "name": "bitte-uniswap-agent",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/app.ts",
    "build": "rm -rf dist && tsc",
    "start:dist": "node dist/src/app.js",
    "dev": "concurrently \"tsx src/app.ts\" \"make-agent dev -p 3000\"",
    "dev-testnet": "concurrently \"tsx src/app.ts\" \"make-agent dev -p 3000 -t\"",
    "serve": "node dist/api/index.js",
    "lint": "eslint '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "eslint --fix '{src,tests}/**/*.{js,jsx,ts,tsx}' && prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config jest.config.ts",
    "vercel-build": "echo hello"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.1.8",
    "@uniswap/sdk-core": "^7.5.0",
    "@uniswap/smart-order-router": "^4.17.9",
    "cors": "^2.8.5",
    "ethers": "5.7.2",
    "express": "^4.21.2",
    "near-safe": "^0.9.10",
    "viem": "^2.22.22",
    "zerion-sdk": "^0.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "make-agent": "^0.2.4",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
