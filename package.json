{
  "name": "bitte-uniswap-agent",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/app.ts",
    "build": "rm -rf dist && tsc",
    "start:dist": "node dist/src/app.js",
    "dev": "concurrently \"tsx src/app.ts\" \"make-agent dev -p 3000\"",
    "dev-testnet": "concurrently \"tsx src/app.ts\" \"make-agent dev -p 3000 -t\"",
    "serve": "node dist/api/index.js",
    "lint": "eslint '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "eslint --fix '{src,tests}/**/*.{js,jsx,ts,tsx}' && prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config jest.config.ts",
    "vercel-build": "echo hello"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.2.0",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/smart-order-router": "^4.22.0",
    "cors": "^2.8.5",
    "ethers": "5.7.2",
    "express": "^4.21.2",
    "near-safe": "^0.10.0",
    "viem": "^2.31.4",
    "zerion-sdk": "^0.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^9.2.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^15.15.0",
    "jest": "^30.0.2",
    "make-agent": "^0.3.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
